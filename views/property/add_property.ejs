<%- include('../include/header'); %>
    <link rel="stylesheet" href="/assets/css/wizard.css">
    <link rel="stylesheet" href="/assets/css/style.css">
    </head>

    <body>
        <%- include('../include/navbar'); %>

            <div class="content-area submit-property" style="background-color: #FCFCFC;">&nbsp;
                <div class="container">
                    <div class="clearfix">
                        <div class="wizard-container">

                            <div class="wizard-card ct-wizard-orange" id="wizardProperty">
                                <form action="" method="">
                                    <div class="wizard-header">
                                        <h3>
                                            <b>Submit</b> YOUR PROPERTY <br>
                                            <small>Lorem ipsum dolor sit amet, consectetur adipisicing.</small>
                                        </h3>
                                    </div>

                                    <ul>
                                        <li><a href="#step1" data-toggle="tab">Step 1 </a></li>
                                        <li><a href="#step2" data-toggle="tab">Step 2 </a></li>
                                        <li><a href="#step3" data-toggle="tab">Step 3 </a></li>
                                        <li><a href="#step4" data-toggle="tab">Finished </a></li>
                                    </ul>

                                    <div class="tab-content">
                                        <div class="tab-pane" id="step1">
                                            <div class="row p-b-15  ">
                                                <h4 class="info-text"> Let's start with the basic information (with
                                                    validation)</h4>
                                                <!-- <div class="col-sm-4 col-sm-offset-1">
                                                    <div class="picture-container">
                                                        <div class="picture">
                                                            <img src="assets/img/default-property.jpg"
                                                                class="picture-src" id="wizardPicturePreview"
                                                                title="" />
                                                            <input type="file" id="wizard-picture">
                                                        </div>
                                                    </div>
                                                </div> -->
                                                <div class="col-sm-12">
                                                    <div class="form-group">
                                                        <label for="propertyTitle">Property Title
                                                            <small>(required)</small></label>
                                                        <input name="propertyTitle" id="propertyTitle" type="text"
                                                            class="form-control" placeholder="Super villa ...">
                                                    </div>
                                                </div>
                                                <div class="col-sm-12">
                                                    <div class="form-group">
                                                        <label for="propertyDesc">Property Description :</label>
                                                        <textarea name="propertyDesc" id="propertyDesc" rows="5"
                                                            class="form-control"></textarea>
                                                    </div>
                                                </div>
                                                <div class="col-sm-4">
                                                    <div class="form-group">
                                                        <label for="propertyAddress1">House no
                                                            <small>(required)</small></label>
                                                        <input name="propertyAddress1" id="propertyAddress1" type="text"
                                                            class="form-control" placeholder="1234 main st">
                                                    </div>
                                                </div>
                                                <div class="col-sm-8">
                                                    <div class="form-group">
                                                        <label for="propertyAddress2">Street,locality </label>
                                                        <input name="propertyAddress2" id="propertyAddress2" type="text"
                                                            class="form-control"
                                                            placeholder="Aparment, Studio or Floor">
                                                    </div>
                                                </div>
                                                <div class="col-sm-4">
                                                    <div class="form-group">
                                                        <label for="propertyCity">City <small>(required)</small></label>
                                                        <input name="propertyCity" id="propertyCity" type="text"
                                                            class="form-control" placeholder="near land mark">
                                                    </div>
                                                </div>
                                                <div class="col-sm-4">
                                                    <div class="form-group">
                                                        <label for="propertyState">State
                                                            <small>(required)</small></label>
                                                        <select name="propertyState" id="propertyState"
                                                            placeholder="near land mark" class="selectpicker"
                                                            data-live-search="true" data-live-search-style="begins">
                                                            <option selected disabled value="">Choose...</option>
                                                            <option>Andhra Pradesh</option>
                                                            <option>Arunachal Pradesh</option>
                                                            <option>Assam</option>
                                                            <option>Andaman and Nicobar Islands</option>
                                                            <option>Bihar</option>
                                                            <option>Chandigarh</option>
                                                            <option>Chhattisgarh</option>
                                                            <option>Dadra and Nagar Haveli and Daman and Diu</option>
                                                            <option>Delhi</option>
                                                            <option>Goa</option>
                                                            <option>Gujrat</option>
                                                            <option>Haryana</option>
                                                            <option>Himachal Pradesh</option>
                                                            <option>Jharkhand</option>
                                                            <option>kerela</option>
                                                            <option>Madhya pradesh</option>
                                                            <option>Maharastra</option>
                                                            <option>Manipur</option>
                                                            <option>Meghalaya</option>
                                                            <option>Mizoram</option>
                                                            <option>Nagaland</option>
                                                            <option>Odisha</option>
                                                            <option>Punjab</option>
                                                            <option>Rajasthan</option>
                                                            <option>Sikkim</option>
                                                            <option>Tamil Nadu</option>
                                                            <option>Telangana</option>
                                                            <option>Tripura</option>
                                                            <option>Uttar Pradesh</option>
                                                            <option>Uttarakhand</option>
                                                            <option>West Bengal</option>
                                                            <option>Jammu and Kashmir</option>
                                                            <option>Ladakh</option>
                                                            <option>Lakshadweep</option>
                                                            <option>Puducherry</option>

                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-sm-4">
                                                    <div class="form-group">
                                                        <label for="propertyPincode">Pincode
                                                            <small>(required)</small></label>
                                                        <input name="propertyPincode" id="propertyPincode" type="text"
                                                            class="form-control" placeholder="near land mark">
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                        <!--  End step 1 -->

                                        <div class="tab-pane" id="step2">
                                            <h4 class="info-text"> How much your Property is Beautiful ? </h4>
                                            <div class="row">


                                                <div class="col-sm-12">
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="providerType">Provider Type :</label>
                                                            <select id="providerType" name="providerType"
                                                                class="selectpicker" data-live-search="true"
                                                                data-live-search-style="begins"
                                                                title="Select your city">
                                                                <option>Seoul</option>
                                                                <option>Paris</option>
                                                                <option>Casablanca</option>
                                                                <option>Tokyo</option>
                                                                <option>Marraekch</option>
                                                                <option>kyoto , shibua</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="propertyAvailableFor">Available For :</label>
                                                            <select id="propertyAvailableFor"
                                                                name="propertyAvailableFor" class="selectpicker"
                                                                data-live-search="true" data-live-search-style="begins"
                                                                title="Select your city">
                                                                <option>New york, CA</option>
                                                                <option>Paris</option>
                                                                <option>Casablanca</option>
                                                                <option>Tokyo</option>
                                                                <option>Marraekch</option>
                                                                <option>kyoto , shibua</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="propertyOccupancy">Occupancy :</label>
                                                            <select id="propertyOccupancy" name="propertyOccupancy"
                                                                class="selectpicker show-tick form-control">
                                                                <option> -Status- </option>
                                                                <option>Rent </option>
                                                                <option>Boy</option>
                                                                <option>used</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="propertyFood">Food/Meals :</label>
                                                            <select id="propertyFood" name="propertyFood"
                                                                class="selectpicker show-tick form-control">
                                                                <option> -Status- </option>
                                                                <option>Rent </option>
                                                                <option>Boy</option>
                                                                <option>used</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="propertyElectricity">Electricity :</label>
                                                            <select id="propertyElectricity" name="propertyElectricity"
                                                                class="selectpicker show-tick form-control">
                                                                <option> -Status- </option>
                                                                <option>Rent </option>
                                                                <option>Boy</option>
                                                                <option>used</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="propertyRoomCooling">Room Cooling :</label>
                                                            <select id="propertyRoomCooling" name="propertyRoomCooling"
                                                                class="selectpicker show-tick form-control">
                                                                <option> -Status- </option>
                                                                <option>Rent </option>
                                                                <option>Boy</option>
                                                                <option>used</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="propertySeats">No Of Seats Available
                                                                <small>(required)</small></label>
                                                            <input name="propertySeats" id="propertySeats" type="text"
                                                                class="form-control" placeholder="near land mark">
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="propertyRent">Rent Per Month/Bed in (INR)
                                                                <small>(required)</small></label>
                                                            <input name="propertyRent" id="propertyRent" type="text"
                                                                class="form-control" placeholder="near land mark">
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="propertyDeposit">Deposit :</label>
                                                            <select id="propertyDeposit" name="propertyDeposit"
                                                                class="selectpicker show-tick form-control">
                                                                <option> -Status- </option>
                                                                <option>Rent </option>
                                                                <option>Boy</option>
                                                                <option>used</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="propertyWaterSupply">Water Supply :</label>
                                                            <select id="propertyWaterSupply" name="propertyWaterSupply"
                                                                class="selectpicker show-tick form-control">
                                                                <option> -Status- </option>
                                                                <option>Rent </option>
                                                                <option>Boy</option>
                                                                <option>used</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="propertyHotWater">Hot Water :</label>
                                                            <select id="propertyHotWater" name="propertyHotWater"
                                                                class="selectpicker show-tick form-control">
                                                                <option> -Status- </option>
                                                                <option>Rent </option>
                                                                <option>Boy</option>
                                                                <option>used</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="propertyDrinkingWaterType">Drinking Water Type
                                                                :</label>
                                                            <select id="propertyDrinkingWaterType"
                                                                name="propertyDrinkingWaterType"
                                                                class="selectpicker show-tick form-control">
                                                                <option> -Status- </option>
                                                                <option>Rent </option>
                                                                <option>Boy</option>
                                                                <option>used</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="propertySafety">Safety
                                                                :</label>
                                                            <select id="propertySafety" name="propertySafety"
                                                                class="selectpicker show-tick form-control">
                                                                <option> -Status- </option>
                                                                <option>Rent </option>
                                                                <option>Boy</option>
                                                                <option>used</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <label for="propertyBedType">Bed Type
                                                                :</label>
                                                            <select id="propertyBedType" name="propertyBedType"
                                                                class="selectpicker show-tick form-control">
                                                                <option> -Status- </option>
                                                                <option>Rent </option>
                                                                <option>Boy</option>
                                                                <option>used</option>

                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-sm-12 padding-top-15">
                                                    <div class="col-sm-4">
                                                        <div class="form-group">
                                                            <label for="propertyTimings">Door Opening Timings :</label>
                                                            <input type="text" class="span2" value=""
                                                                data-slider-min="0" data-slider-max="24"
                                                                data-slider-step="1" data-slider-value="[6,20]"
                                                                id="min-bed"><br />
                                                            <b class="pull-left color">0</b>
                                                            <b class="pull-right color">24</b>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-sm-12 padding-top-15">
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <div class="checkbox">
                                                                <label for="propertyParking">
                                                                    <input type="checkbox" name="propertyParking"
                                                                        id="propertyParking"> Parking
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <div class="checkbox">
                                                                <label for="propertyLocker">
                                                                    <input type="checkbox" name="propertyLocker"
                                                                        id="propertyLocker"> Locker
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-sm-3">
                                                        <div class="form-group">
                                                            <div class="checkbox">
                                                                <label for="propertyTable">
                                                                    <input type="checkbox" name="propertyTable"
                                                                        id="propertyTable"> Table/Chair
                                                                </label>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                                <br>
                                            </div>
                                        </div>
                                        <!-- End step 2 -->

                                        <div class="tab-pane" id="step3">
                                            <h4 class="info-text">Give us somme images and videos ? </h4>
                                            <div class="row">
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label for="property-images">Chose Images :</label>
                                                        <input class="form-control" type="file" id="property-images">
                                                        <p class="help-block">Select multipel images for your property .
                                                        </p>
                                                    </div>
                                                </div>
                                                <div class="col-sm-6">
                                                    <div class="form-group">
                                                        <label for="property-video">Property video :</label>
                                                        <input class="form-control" value=""
                                                            placeholder="http://www.youtube.com, http://vimeo.com"
                                                            name="property_video" type="text">
                                                    </div>

                                                    <div class="form-group">
                                                        <input class="form-control" value=""
                                                            placeholder="http://www.youtube.com, http://vimeo.com"
                                                            name="property_video" type="text">
                                                    </div>

                                                    <div class="form-group">
                                                        <input class="form-control" value=""
                                                            placeholder="http://www.youtube.com, http://vimeo.com"
                                                            name="property_video" type="text">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!--  End step 3 -->


                                        <div class="tab-pane" id="step4">
                                            <h4 class="info-text"> Finished and submit </h4>
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    <div class="">
                                                        <p>
                                                            <label><strong>Terms and Conditions</strong></label>
                                                            By accessing or using GARO ESTATE services, such as
                                                            posting your property advertisement with your personal
                                                            information on our website you agree to the
                                                            collection, use and disclosure of your personal information
                                                            in the legal proper manner
                                                        </p>

                                                        <div class="checkbox">
                                                            <label>
                                                                <input type="checkbox" /> <strong>Accept termes and
                                                                    conditions.</strong>
                                                            </label>
                                                        </div>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!--  End step 4 -->

                                    </div>

                                    <div class="wizard-footer">
                                        <div class="pull-right">
                                            <input type='button' class='btn btn-next btn-primary' name='next'
                                                value='Next' />
                                            <input type='button' class='btn btn-finish btn-primary ' name='finish'
                                                value='Finish' />
                                        </div>

                                        <div class="pull-left">
                                            <input type='button' class='btn btn-previous btn-default' name='previous'
                                                value='Previous' />
                                        </div>
                                        <div class="clearfix"></div>
                                    </div>
                                </form>
                            </div>
                            <!-- End submit form -->
                        </div>
                    </div>
                </div>
            </div>

            <script src="/assets/js/vendor/modernizr-2.6.2.min.js"></script>
            <script src="/assets/js//jquery-1.10.2.min.js"></script>
            <script src="/bootstrap/js/bootstrap.min.js"></script>
            <script src="/assets/js/bootstrap-select.min.js"></script>
            <script src="/assets/js/bootstrap-hover-dropdown.js"></script>
            <script src="/assets/js/easypiechart.min.js"></script>
            <script src="/assets/js/jquery.easypiechart.min.js"></script>
            <script src="/assets/js/owl.carousel.min.js"></script>
            <script src="/assets/js/wow.js"></script>
            <script src="/assets/js/icheck.min.js"></script>
            <script src="/assets/js/price-range.js"></script>
            <script src="/assets/js/jquery.bootstrap.wizard.js" type="text/javascript"></script>
            <script src="/assets/js/jquery.validate.min.js"></script>
            <script src="/assets/js/wizard.js"></script>
            <script src="/assets/js/main.js"></script>
    </body>

    </html>