            <%for (const key in master){%>                
                <div class="col-sm-12 col-md-3 mb-2">
                    <a href="/admin/master?category=<%=key%>" class="btn btn-block btn-primary text-capitalize"><%=key%></a>                                                        
                    
                </div>
            <%}%>
